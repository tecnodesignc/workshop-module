{
  "name": "tecnodesignc/workshop-module",
  "type": "encore-module",
  "description": "Workshop module for EncoreCMS. Handles the activation of modules.",
  "keywords": [
    "encorecms",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Rativa",
      "email": "supoert@tecnodesign.com.co",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "supoert@tecnodesign.com.co",
    "issues": "https://github.com/tecnodesignc/Platform/issues",
    "source": "https://github.com/tecnodesignc/Workshop"
  },
  "require": {
    "php": ">=7.1.3",
    "composer/installers": "~1.0",
    "symfony/yaml": "^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.0",
    "orchestra/testbench": "3.8.*",
    "spatie/phpunit-snapshot-assertions": "^1.0"
  },
  "autoload-dev": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "Modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
