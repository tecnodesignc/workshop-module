{
  "name": "tecnodesignc/workshop-module",
  "type": "encore-module",
  "description": "Workshop module for EncoreCMS 3.0. Handles the activation of modules.",
  "keywords": [
    "encorecms",
    "workshop",
    "modules"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Marcos Rativa",
      "email": "info@tecnodesign.com.co",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "supoert@tecnodesign.com.co",
    "issues": "https://github.com/tecnodesignc/encorecms/issues",
    "source": "https://github.com/tecnodesignc/Wworkshop-module"
  },
  "require": {
    "php": ">=8.1",
    "composer/installers": "^2.1",
    "symfony/yaml": "^6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "orchestra/testbench": "3.8.*",
    "spatie/phpunit-snapshot-assertions": "^5.0.1"
  },
  "autoload": {
    "psr-4": {
      "Modules\\Workshop\\": ".",
      "Modules\\": "modules/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "2.1.x-dev"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
